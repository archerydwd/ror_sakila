<h1>Listing film_actors</h1>

<table>
  <thead>
    <tr>
      <th>Actor</th>
      <th>Film</th>
      <th>Last update</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @film_actors.each do |film_actor| %>
      <tr>
        <td><%= film_actor.actor %></td>
        <td><%= film_actor.film %></td>
        <td><%= film_actor.last_update %></td>
        <td><%= link_to 'Show', film_actor %></td>
        <td><%= link_to 'Edit', edit_film_actor_path(film_actor) %></td>
        <td><%= link_to 'Destroy', film_actor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Film actor', new_film_actor_path %>
